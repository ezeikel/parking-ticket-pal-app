{
  "expo": {
    "name": "PCNs - Manage, Track, and Challenge Parking & Penalty Charge Notices with Ease",
    "slug": "pcns",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ezeikel.pcns"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ezeikel.pcns"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["react-native-document-scanner-plugin", {
        "cameraPermission": "We need camera access, so you can scan documents"
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "93b42738-f8ca-4780-b1a7-eb966c8beb4a"
      }
    },
    "owner": "ezeikel",
    "updates": {
      "url": "https://u.expo.dev/93b42738-f8ca-4780-b1a7-eb966c8beb4a"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
